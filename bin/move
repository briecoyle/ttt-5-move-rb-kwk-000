#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"

def input_to_index
  input = gets.strip
  index = #{input}to.i - 1
end

input_to_index

move(index)
